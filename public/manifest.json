{
  "manifest_version": 3,
  "name": "Quercus Scraper",
  "description": "Downloads material from Quercus.",
  "version": "1.0.0",
  "icons": {
    "16": "icon_128.png",
    "48": "icon_128.png",
    "128": "icon_128.png"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./static/js/content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"]
  }